x++;
}
//initialize an instance of composite
MovAve
stream<float x> P=MovAver(S)(N);
Sink(P){
work{
print(P[0].x);
}
}
}

composite MovAver(output O,
input rawIn){
param
int N;
graph
stream<float x> O=Aver(rawIn){
float w[N];
init{
for(i=0;i<N;i++)
w[i]=i;}
work{
int sum=0, i;
for(i=0;i<N;i++)
sum += rawIn[i].x*w[i];
O[0].i=sum/N;
}
window{
rawIn sliding(N,1)
O tumbling(1);
}}}

